{
  "expo": {
    "name": "React Native MobX Boilerplate",
    "description": "A React Native boilerplate with Expo, MobX with persistance, and React Navigation. Repo at https://github.com/ChronSyn/react-native-mobx-boilerplate",
    "slug": "react-native-mobx-boilerplate",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "version": "0.0.0.1",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "./assets/icons/app.png",
    "loading": {
      "icon": "./assets/icons/loading.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mycompany.mypackage",
      "versionCode": 1,
      "config" : {
        "googleMaps" : {
          "apiKey" : "ADD YOUR GOOGLE MAPS API KEY HERE IF USING THAT API"
        }
      }
    },
    "androidStatusBar" : {
      "barStyle" : "light-content",
      "backgroundColor" : "#000000"
    },
    "splash" : {
      "backgroundColor" : "#000000",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    }
  }
}
